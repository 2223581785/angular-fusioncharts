<% if(type == 'js') { %>
$scope.dataSource = {
    chart: {
        caption: "Harry's SuperMart",
        subCaption: "Top 5 stores in last month by revenue",
    },
    data:[{
        label: "Bakersfield Central",
        value: "880000"
    },
    {
        label: "Garden Groove harbour",
        value: "730000"
    },
    {
        label: "Los Angeles Topanga",
        value: "590000"
    },
    {
        label: "Compton-Rancho Dom",
        value: "520000"
    },
    {
        label: "Daly City Serramonte",
        value: "330000"
    }]
};

$scope.updateMyChartData = function () {
    $scope.dataSource.chart.bgColor = "#ffffff";
};
<% } else { %>
<div fc-chart
    fc-width="600" 
    fc-height="400"
    fc-type="column2d",
    fc-datasource="{{dataSource}}"
    ></div>
<p><a class="btn btn-default" ng-click="updateMyChartData()">Change chart background color</a></p>
<% } %>